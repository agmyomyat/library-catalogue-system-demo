{
  "name": "nestjs-nextjs-mvc-starter",
  "private": true,
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "nest": "nest",
    "next": "cd src/client && next",
    "prebuild": "rimraf dist",
    "build": "NODE_ENV=production nest build && cd src/client && next build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch --preserveWatchOutput",
    "start:debug": "nest start --debug --watch --preserveWatchOutput",
    "start:prod": "NODE_ENV=production node dist/src/server/main",
    "lint": "eslint \"{src,test}/**/*.{ts,tsx}\"",
    "test": "jest --setupFiles dotenv-flow/config --collect-coverage",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:request": "jest --setupFiles dotenv-flow/config --config ./test/jest-request.json"
  },
  "dependencies": {
    "@babel/runtime": "^7.23.2",
    "@nestjs/common": "^9.1.6",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^9.1.6",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/platform-express": "^9.4.3",
    "commander": "^11.1.0",
    "cookie-parser": "^1.4.6",
    "nestjs-console": "^9.0.0",
    "nanoid": "5.0.3",
    "@prisma/client": "5.6.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.5",
    "rxjs": "^7.8.1",
    "ts-morph": "^20.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.3.1",
    "@nestjs/cli": "^10.2.1",
    "@nestjs/schematics": "^10.0.3",
    "@nestjs/testing": "^9.2.1",
    "prisma": "^5.6.0",
    "@types/cookie-parser": "^1.4.6",
    "@types/express": "^4.17.21",
    "@types/jest": "^28.1.8",
    "@types/node": "^20.9.0",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "dotenv-flow": "^4.0.1",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "fishery": "^2.2.2",
    "jest": "^28.1.3",
    "prettier": "^3.1.0",
    "supertest": "^6.3.3",
    "ts-jest": "^28.0.8",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.1",
    "typescript": "^5.2.2"
  }
}
